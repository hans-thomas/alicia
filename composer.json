{
    "name": "hans-thomas/alicia",
    "description": "it's an uploader and converter for pictures and videos",
    "keywords": [
        "file-manager",
        "hls-live-streaming",
        "file-uploader",
        "upload-videos",
        "upload-images",
        "upload-files"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "mortazavi",
            "email": "s.mohammad027@live.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-exif": "*",
        "spatie/laravel-image-optimizer": "^1.7",
        "pbmedia/laravel-ffmpeg": "^8",
        "spatie/image": "^2.2"
    },
    "require-dev": {
      "orchestra/testbench": "^8.0",
      "nunomaduro/collision": "^7.0"
    },
    "autoload": {
      "psr-4": {
        "Hans\\Alicia\\": "src/"
      },
      "files": [
        "./src/Helpers/functions.php"
      ]
    },
    "autoload-dev": {
        "psr-4": {
            "Hans\\Alicia\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hans\\Alicia\\AliciaServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ],
        "testbench": "./vendor/bin/testbench",
        "test": "./vendor/bin/phpunit",
        "test:testbench": "./vendor/bin/testbench package:test",
        "test:coverage": "./vendor/bin/phpunit --coverage-clover ./coverage.xml"
    }
}
